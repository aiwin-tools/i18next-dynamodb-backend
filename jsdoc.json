{
  "plugins": [],
  "recurseDepth": 10,
  "source": {
    "include": [
      "src"
    ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\\\/|\\\\\\\\)((node_modules)|(docs))",
    "exlude": [
      "node_modules",
      "dist"
    ]
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "opts": {
    "recurse": true
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": false
  }
}
